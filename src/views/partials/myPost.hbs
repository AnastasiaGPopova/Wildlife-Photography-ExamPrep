                    <div class="my-card">
                        <div class="card-header">
                            <img src={{imageUrl}} alt="water_fall" />
                        </div>
                        <div class="card-body">
                            <span class="tag tag-teal">Keyword: {{keyword}}</span>
                            <h4>
                                Title: {{title}}
                            </h4>
                            <div class="user">
                                <img src={{imageUrl}} alt="vote" />
                                <div class="user-info">
                                    <h5>Author: {{autor.firstName}} {{autor.lastName}}</h5>
                                    <small>Rating of votes: {{rating}}</small>
                                </div>
                            </div>
                        </div>
                    </div>